   "source": [
    "#Using a for loop to access elements in an iterator\n",
    "l1=[1,2,3,4,5]\n",
    "i=iter(l1)\n",
    "for n in l1:\n",
    "    print(next(i))\n",
    "#using while loop to use the iterator\n",
    "l2=['sainadh',13,15]\n",
    "i1=iter(l2)\n",
    "while True:\n",
    "    try:\n",
    "        a=next(i1)\n",
    "        print(a)\n",
    "    except StopIteration:\n",
    "        break"
   ]
