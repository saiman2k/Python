   "source": [
    "#numpy iterating over an array:\n",
    "import numpy as np\n",
    "a=np.arange(0,100,10)\n",
    "a=a.reshape(2,5)\n",
    "print('Original array is:')\n",
    "print(a)\n",
    "print('Transpose of the original array is:')\n",
    "b=a.T\n",
    "print(b)\n",
    "print('Modified array is:')\n",
    "for x in np.nditer(b):\n",
    "    print(x,)"
   ]
