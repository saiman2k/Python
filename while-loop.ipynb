    "#while loop to find the factorial\n",
    "print('Enter the number:')\n",
    "a=int(input())\n",
    "i=1\n",
    "m=1\n",
    "while i<=a:\n",
    "    m=m*i\n",
    "    i=i+1\n",
    "print('The factorial of number is:',m)"
